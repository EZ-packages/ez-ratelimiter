  },
  "homepage": "https://github.com/ezier-project/ratelimit#readme",
  "devDependencies": {
    "@swc-node/register": "^1.4.2",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "chai": "^4.3.6",
    "mocha": "^9.2.2",
    "prettier": "^2.6.1",
    "tsconfig-paths": "^4.1.2",
    "typedoc": "^0.22.13"
  },
  "files": [
    "dist/**/*"
